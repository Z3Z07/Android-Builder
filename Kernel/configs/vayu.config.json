[
  {
    "kernelSource": {
      "name": "vayu_kernel",
      "repo": "https://github.com/MiCode/Xiaomi_Kernel_OpenSource.git",
      "branch": "vayu-r-oss",
      "device": "vayu",
      "defconfig": "vayu_user_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
		"PATH": "proton-clang/bin:${path}",
		"SUBARCH": "arm64",
		"COMPILER": "clang",
		"CC": "clang",
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
		"LD_LIBRARY_PATH": "proton-clang/lib",
		"LD": "ld.lld",
		"AR": "llvm-ar",
		"NM": "llvm-nm",
		"OBJCOPY": "llvm-objcopy",
		"OBJDUMP": "llvm-objdump",
		"STRIP": "llvm-strip",
		"ld-name": "lld",
		"KBUILD_COMPILER_STRING": "Proton Clang"
      }
    },
	"AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
