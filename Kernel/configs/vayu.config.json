[
  {
    "kernelSource": {
      "name": "vayu_kernel",
      "repo": "https://github.com/MiCode/Xiaomi_Kernel_OpenSource.git",
      "branch": "vayu-r-oss",
      "device": "vayu",
      "defconfig": "vayu_user_defconfig"
    },
    "toolchains": [
      {
        "name": "aosp-clang",
        "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86",
        "branch": "android11-release",
        "binaryEnv": ["/bin"]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "PATH": "aosp-clang/bin:${PATH}",
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
		"CROSS_COMPILE_COMPAT": "arm-linux-androideabi-",
		"DTC_EXT": "dtc"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
