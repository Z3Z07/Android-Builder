[
  {
    "kernelSource": {
      "name": "vayu_kernel",
      "repo": "https://github.com/Z3Z07/vayu_kernel.git",
      "branch": "master",
      "device": "vayu",
      "defconfig": "vayu_user_defconfig"
    },
    "toolchains": [
       {
         "name": "clang",
         "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/android-11.0.0_r1/clang-r383902b.tar.gz" ,
         "binaryEnv": ["./bin"]
       },
       {
         "name": "gcc",
         "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-11.0.0_r1.tar.gz",
         "binaryEnv": ["bin"]
       }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
