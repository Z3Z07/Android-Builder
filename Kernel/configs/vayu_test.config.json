[
    {
      "kernelSource": {
        "name": "vayu_test",
        "repo": "https://github.com/MiCode/Xiaomi_Kernel_OpenSource.git",
        "branch": "vayu-r-oss",
        "device": "vayu",
        "defconfig": "vayu_user_defconfig"
      },
      "toolchains": [
   {
     "name": "Clang-16.0.6-20231003.tar.gz",
     "url": "https://github.com/ZyCromerZ/Clang/releases/download/16.0.6-20231003-release/Clang-16.0.6-20231003.tar.gz" ,
     "binaryEnv": ["./bin"]
   }
   ],
      "enableCcache": true,
      "params": {
        "ARCH": "arm64",
        "CC": "Clang-16.0.6-20231003.tar.gz/bin/clang",
        "externalCommands": {
          "CROSS_COMPILE": "aarch64-linux-gnu-",
          "CLANG_TRIPLE": "aarch64-linux-gnu-",
          "NM":"llvm-nm",
          "OBJDUMP":"llvm-objdump",
          "STRIP":"llvm-strip"
        }
      },
      "AnyKernel3": {
        "use": true,
        "release": true
      },
      "enableKernelSU": false,
      "enableLXC": false
    }
]
