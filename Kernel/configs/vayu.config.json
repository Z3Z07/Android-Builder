[
  {
    "kernelSource": {
      "name": "vayu_kernel",
      "repo": "https://github.com/MiCode/Xiaomi_Kernel_OpenSource.git",
      "branch": "vayu-r-oss",
      "device": "vayu",
      "defconfig": "vayu_user_defconfig"
    },
    "toolchains": [
      {
        "name": "gcc",
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android11-release",
        "binaryEnv": ["./bin"]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "PATH": "aosp-clang/bin:${PATH}",
        "CROSS_COMPILE": "aarch64-linux-android-",
		"DTC_EXT": "dtc"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
